{
  "name": "pouchdb-upsert-bulk",
  "version": "7.5.0",
  "description": "Upsert bulk plugin for PouchDB",
  "mainOrg": "dist/pouchdb-upsert-build.cjs.js",
  "moduleOrg": "dist/pouchdb-upsert-build.es6.js",
  "esnextOrg": "dist/pouchdb-upsert-build.esnext.js",
  "browserOrg": "dist/pouchdb-upsert-build.browser.js",
  "main": "lib/index.js",
  "jsnext:main": "lib/index.es.js",
  "module": "lib/index.es.js",
  "browser": "lib/index.browser.js",
  "types": "types/index.d.ts",
  "brVars": {
    "name": "PouchUpsertBulk",
    "package": "pouchdb.upsert.bulk"
  },
  "scripts": {
    "build:prod": "npm-run-all clean rollup browserify minify",
    "build:dev": "npm-run-all clean rollup browserify",
    "build": "npm run build:prod",
    "buildOrg": "rollup --config config/rollup.config.js && rollup --config config/rollup.config.es6.js && rollup --config config/rollup.config.esnext.js",
    "clean": "rimraf dist/* lib/* compiled/* types/* && mkdirp dist lib compiled types",
    "compile": "tsc -p ./tsconfig.json",
    "minify": "uglifyjs -mc < dist/$npm_package_brVars_package.js > dist/$npm_package_brVars_package.min.js",
    "rollup-cjs-old": "rollup -c config/rollup.config.cjs.js && rollup -c config/rollup.config.browser.cjs.js",
    "rollup-cjs": "rollup -c config/rollup.config.cjs.js",
    "rollup-es": "rollup -c config/rollup.config.esnext.js && rollup -c config/rollup.config.browser.js",
    "rollup": "npm-run-all --parallel rollup-cjs rollup-es",
    "browserify:rollup": "browserify -t [ babelify --global --presets [ @babel/preset-env ] ] -t brfs -p bundle-collapser/plugin -s $npm_package_brVars_name lib/index.browser.js > dist/$npm_package_brVars_package.js",
    "browserify": "npm run browserify:rollup",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "pretest": "standard",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm run build && npm publish && semantic-release post"
  },
  "scriptsOriginal": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "rollup --config && rollup --config rollup.config.es6.js && rollup --config rollup.config.esnext.js",
    "prepublish": "npm run build",
    "pretest": "standard",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm run build && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chorpler/pouchdb-upsert-bulk.git"
  },
  "keywords": [
    "pouchdb",
    "upsert",
    "bulk"
  ],
  "author": "David Sargeant <chorpler@gmail.com>",
  "contributors": [
    "Tom Vincent <git@tlvince.com> (https://tlvince.com)"
  ],
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/chorpler/pouchdb-upsert-bulk/issues"
  },
  "homepage": "https://github.com/chorpler/pouchdb-upsert-bulk#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@types/node": "14.18.23",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-core": "^7.0.10",
    "@types/pouchdb-find": "^6.3.7",
    "@types/pouchdb-http": "^6.1.3",
    "@types/pouchdb-replication": "^6.4.4",
    "ava": "4.3.1",
    "babelify": "10.0.0",
    "brfs": "2.0.2",
    "browserify": "17.0.0",
    "builtin-modules": "3.3.0",
    "bundle-collapser": "1.4.0",
    "chokidar": "3.5.3",
    "cross-env": "7.0.3",
    "fsevents": "2.3.2",
    "ghooks": "2.0.4",
    "mkdirp": "1.0.4",
    "npm-run-all": "4.1.5",
    "pouchdb-http": "6.0.2",
    "rimraf": "3.0.2",
    "rollup": "1.26.3",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-buble": "0.14.0",
    "rollup-plugin-typescript2": "0.32.1",
    "semantic-release": "19.0.3",
    "standard": "17.0.0",
    "typescript": "3.9.10",
    "uglify-js": "3.16.3",
    "validate-commit-msg": "2.14.0"
  },
  "files": [
    "dist"
  ],
  "configOriginal": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-push": "npm test"
    }
  },
  "license": "MIT"
}
