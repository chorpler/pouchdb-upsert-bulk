{
  "name": "pouchdb-upsert-bulk",
  "version": "7.5.0",
  "description": "Upsert bulk plugin for PouchDB",
  "main": "dist/pouchdb-upsert-build.cjs.js",
  "module": "dist/pouchdb-upsert-build.es6.js",
  "esnext": "dist/pouchdb-upsert-build.esnext.js",
  "scripts": {
    "clean": "rimraf dist/* compiled/* && mkdirp dist compiled",
    "compile": "tsc -p ./tsconfig.json",
    "prebuild": "npm run clean",
    "build": "rollup --config config/rollup.config.js && rollup --config config/rollup.config.es6.js && rollup --config config/rollup.config.esnext.js",
    "prepublish": "npm run build",
    "pretest": "standard",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm run build && npm publish && semantic-release post"
  },
  "scriptsOriginal": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "rollup --config && rollup --config rollup.config.es6.js && rollup --config rollup.config.esnext.js",
    "prepublish": "npm run build",
    "pretest": "standard",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm run build && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chorpler/pouchdb-upsert-bulk.git"
  },
  "keywords": [
    "pouchdb",
    "upsert",
    "bulk"
  ],
  "author": "David Sargeant <chorpler@gmail.com>",
  "contributors": [
    "Tom Vincent <git@tlvince.com> (https://tlvince.com)"
  ],
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/chorpler/pouchdb-upsert-bulk/issues"
  },
  "homepage": "https://github.com/chorpler/pouchdb-upsert-bulk#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-core": "^7.0.10",
    "@types/pouchdb-find": "^6.3.7",
    "@types/pouchdb-http": "^6.1.3",
    "@types/pouchdb-replication": "^6.4.4",
    "ava": "4.3.1",
    "builtin-modules": "3.3.0",
    "chokidar": "3.5.3",
    "cross-env": "7.0.3",
    "fsevents": "2.3.2",
    "ghooks": "2.0.4",
    "mkdirp": "1.0.4",
    "pouchdb-http": "6.0.2",
    "rimraf": "3.0.2",
    "rollup": "0.36.4",
    "rollup-plugin-buble": "0.14.0",
    "rollup-plugin-typescript2": "0.32.1",
    "semantic-release": "19.0.3",
    "standard": "17.0.0",
    "typescript": "3.9.10",
    "validate-commit-msg": "2.14.0"
  },
  "files": [
    "dist"
  ],
  "configOriginal": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-push": "npm test"
    }
  },
  "license": "MIT"
}
